[
  {
    "name": "Welcome",
    "settings": [
      {
        "type": "paragraph",
        "content": "Theme settings allow designers to provide an easy way for any user to customize the look and feel of their shop without having to delve into HTML or CSS code. Please see our [help page on theme settings](http:\/\/docs.shopify.com\/themes\/theme-templates\/settings) to learn more about how you can make your theme customizable."
      }
    ]
  },
  {
    "name": "Favicon",
    "settings": [
      {
        "type": "checkbox",
        "id": "use_favicon",
        "label": "Use a custom icon"
      },
      {
        "type": "image",
        "id": "favicon.png",
        "max-width": 16,
        "max-height": 16,
        "label": "Favicon",
        "info": "16 x 16px required"
      }
    ]
  },
  {
    "name": "Newsletter",
    "settings": [
      {
        "type": "header",
        "content": "Popup Newsletter"
      },
      {
        "type": "checkbox",
        "id": "display_popup_newsletter",
        "label": "Display Popup Newsletter?",
        "default": true
      },
      {
        "type": "image",
        "id": "bg_newsletter.jpg",
        "label": "Upload a new newsletter image",
        "max-width": 2000,
        "max-height": 2000
      },
      {
        "type": "text",
        "id": "popup_newsletter_title",
        "label": "Popup Newsletter Title"
      },
      {
        "type": "text",
        "id": "popup_newsletter_sub_title",
        "label": "Popup Newsletter Sub Title"
      },
      {
        "type": "text",
        "id": "popup_newsletter_message",
        "label": "Popup Newsletter Message"
      },
      {
        "type": "text",
        "id": "mailing_list_form_action",
        "label": "MailChimp form action URL",
        "info": "[Where do I find my MailChimp form action URL?](http:\/\/docs.shopify.com\/support\/configuration\/store-customization\/where-do-i-get-my-mailchimp-form-action)"
      }
    ]
  },
  {
    "name": "Background",
    "settings": [
      {
        "type": "radio",
        "id": "bg_body_img",
        "label": "Background image",
        "options": [
          {
            "value": "bg-body.png",
            "label": "Uploaded image"
          },
          {
            "value": "none",
            "label": "Solid color (no image)"
          }
        ]
      },
      {
        "type": "color",
        "id": "bg_body_color",
        "label": "Background color",
        "default": "#fff"
      },
      {
        "type": "radio",
        "id": "bg_body_position_x",
        "label": "Horizontal position",
        "options": [
          {
            "value": "left",
            "label": "Left"
          },
          {
            "value": "center",
            "label": "Center"
          },
          {
            "value": "right",
            "label": "Right"
          }
        ]
      },
      {
        "type": "radio",
        "id": "bg_body_position_y",
        "label": "Vertical position",
        "options": [
          {
            "value": "top",
            "label": "Top"
          },
          {
            "value": "center",
            "label": "Center"
          },
          {
            "value": "bottom",
            "label": "Bottom"
          }
        ]
      },
      {
        "type": "select",
        "id": "bg_body_repeat",
        "label": "Repeat",
        "options": [
          {
            "value": "repeat",
            "label": "Horizontally and vertically"
          },
          {
            "value": "repeat-x",
            "label": "Horizontally only"
          },
          {
            "value": "repeat-y",
            "label": "Vertically only"
          },
          {
            "value": "no-repeat",
            "label": "Don't repeat"
          }
        ]
      },
      {
        "type": "select",
        "id": "bg_body_attachment",
        "label": "Attachment",
        "options": [
          {
            "value": "scroll",
            "label": "Scroll"
          },
          {
            "value": "fixed",
            "label": "Fixed"
          }
        ]
      },
      {
        "type": "image",
        "id": "bg-body.png",
        "label": "Upload a new image",
        "max-width": 2000,
        "max-height": 2000
      }
    ]
  },
